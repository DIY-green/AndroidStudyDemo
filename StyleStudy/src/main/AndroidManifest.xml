<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cheng.stylestudy" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RectangleActivity"
            android:label="@string/title_activity_rectangle" />
        <activity
            android:name=".OvalActivity"
            android:label="@string/title_activity_oval" />
        <activity
            android:name=".LineActivity"
            android:label="@string/title_activity_line" />
        <activity
            android:name=".RingActivity"
            android:label="@string/title_activity_ring" />
        <activity
            android:name=".ShapeActivity"
            android:label="@string/title_activity_shape" >
        </activity>
        <activity
            android:name=".SelectorActivity"
            android:label="@string/title_activity_selector" >
        </activity>
        <activity
            android:name=".LayerListActivity"
            android:label="@string/title_activity_layer_list" >
        </activity>
    </application>

</manifest>
